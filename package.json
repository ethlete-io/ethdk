{
  "name": "@ethlete/root",
  "version": "0.0.0",
  "private": true,
  "license": "GPL-3.0",
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "change": "bun changeset",
    "changeset:prerelease": "node ./tools/scripts/enter-prerelease-mode.js && bun changeset version && bun install --mode=update-lockfile",
    "changeset:release": "node ./tools/scripts/exit-prerelease-mode.js && bun changeset version && bun install --mode=update-lockfile",
    "commit": "bunx git-cz",
    "fix-deps": "rm -f .eslintcache && bun nx run-many -t lint --skip-nx-cache --fix && bun install",
    "format": "nx format:write --base=origin/main --head=HEAD",
    "prepare:release": "node ./tools/scripts/prepare-release.js",
    "release": "bun prepare:release && bun changeset publish",
    "start": "nx serve playground",
    "storybook": "nx run playground:storybook --no-open",
    "update-nx": "rm -f migrations.json && bunx nx migrate latest && bunx nx migrate --run-migrations --if-exists"
  },
  "dependencies": {
    "@angular/animations": "18.0.6",
    "@angular/cdk": "18.0.6",
