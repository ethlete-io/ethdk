{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/libs/cdk",
  "assets": [
    "CHANGELOG.md",
    "./src/lib/styles/**/*",
    "generators/generators.json",
    {
      "glob": "**/*.js",
      "input": "generators/dist",
      "output": "./generators"
    },
    {
      "glob": "**/schema.json",
      "input": "generators",
      "output": "./generators"
    }
  ],
  "lib": {
    "entryFile": "src/index.ts"
  }
}
