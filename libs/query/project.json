{
  "name": "query",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/query/src",
  "prefix": "et",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "dependsOn": ["^build", "build-generators"],
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/query/ng-package.json",
        "tsConfig": "libs/query/tsconfig.lib.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/query/tsconfig.lib.prod.json"
        },
        "development": {}
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/query/jest.config.ts",
        "tsConfig": "libs/query/tsconfig.spec.json"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "build-generators": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf libs/query/generators/dist", "tsc -p libs/query/generators/tsconfig.json"]
      }
    }
  }
}
